{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/audio/(.*)",
      "dest": "/audio/$1"
    },
    {
      "src": "/select_language",
      "methods": ["POST"],
      "dest": "app.py"
    },
    {
      "src": "/get_transcription",
      "methods": ["GET"],
      "dest": "app.py"
    },
    {
      "src": "/get_text_content",
      "methods": ["GET"],
      "dest": "app.py"
    },
    {
      "src": "/get_audio_path",
      "methods": ["POST"],
      "dest": "app.py"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
